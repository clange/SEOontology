# Turn off MultiViews
Options -MultiViews

# Directive to ensure *.rdf files served as appropriate content type,
# if not present in main apache config
AddType application/rdf+xml .rdf

# Rewrite engine setup
RewriteEngine On
RewriteBase /eventsoo

# Rewrite rule to redirect 303 from any class or prop URI
RewriteRule ^eventsoo/.+ eventsoo/ [R=303]

# Rewrite rule to serve RDF/XML content from the vocabulary URI
RewriteRule ^events/$ events.rdf
