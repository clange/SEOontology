<?xml version="1.0" encoding="utf-8" ?>
<rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:owl="http://www.w3.org/2002/07/owl#"
         xmlns:dc11="http://purl.org/dc/elements/1.1/"
         xmlns:dc="http://purl.org/dc/terms/"
         xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#">

  <owl:Ontology rdf:about="https://purl/eventsoo/">
    <owl:imports rdf:resource="https://w3id.org/scholarlydata/ontology/conference-ontology.owl"/>
    <dc11:description xml:lang="en">Said Fathalla</dc11:description>
    <dc11:title xml:lang="en">EVENTSO, a scientific events ontology.</dc11:title>
    <dc:lastModified xml:lang="en">2018-03-28</dc:lastModified>
  </owl:Ontology>

  <owl:ObjectProperty rdf:about="http://purl.org/eventsoo/belongsToSeries">
    <rdfs:domain rdf:resource="https://w3id.org/scholarlydata/ontology/conference-ontology.owl#AcademicEvent"/>
    <rdfs:range rdf:resource="https://w3id.org/scholarlydata/ontology/conference-ontology.owl#EventSeries"/>
    <rdfs:comment xml:lang="en">The series which an eveny belongs to</rdfs:comment>
    <rdfs:label xml:lang="en">belongsToSeries</rdfs:label>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="http://purl.org/eventsoo/colocatedWith">
  </owl:ObjectProperty>

  <owl:DatatypeProperty rdf:about="http://purl.org/eventsoo/acceptanceRate">
    <rdfs:domain rdf:resource="https://w3id.org/scholarlydata/ontology/conference-ontology.owl#AcademicEvent"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#decimal"/>
    <rdfs:comment xml:lang="en">The acceptance rate of the event in a particular year.</rdfs:comment>
  </owl:DatatypeProperty>

  <owl:DatatypeProperty rdf:about="http://purl.org/eventsoo/acceptedPapers">
    <rdfs:domain rdf:resource="https://w3id.org/scholarlydata/ontology/conference-ontology.owl#AcademicEvent"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#unsignedInt"/>
    <rdfs:comment xml:lang="en">The number of accepted papers in the event.</rdfs:comment>
  </owl:DatatypeProperty>

  <owl:DatatypeProperty rdf:about="http://purl.org/eventsoo/city">
    <rdfs:domain rdf:resource="https://w3id.org/scholarlydata/ontology/conference-ontology.owl#AcademicEvent"/>
    <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
    <rdfs:comment xml:lang="en">The city in which the event takes place.</rdfs:comment>
  </owl:DatatypeProperty>

  <owl:DatatypeProperty rdf:about="http://purl.org/eventsoo/country">
    <rdfs:domain rdf:resource="https://w3id.org/scholarlydata/ontology/conference-ontology.owl#AcademicEvent"/>
    <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
    <rdfs:comment xml:lang="en">The country hosts the event.</rdfs:comment>
  </owl:DatatypeProperty>

  <owl:DatatypeProperty rdf:about="http://purl.org/eventsoo/field">
    <rdfs:domain rdf:resource="https://w3id.org/scholarlydata/ontology/conference-ontology.owl#AcademicEvent"/>
    <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
    <rdfs:comment xml:lang="en">The main research field of the event (e.g. Database systems)</rdfs:comment>
  </owl:DatatypeProperty>

  <owl:DatatypeProperty rdf:about="http://purl.org/eventsoo/homepage">
    <rdfs:domain rdf:resource="https://w3id.org/scholarlydata/ontology/conference-ontology.owl#AcademicEvent"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#anyURI"/>
    <rdfs:comment xml:lang="en">The home page of the event</rdfs:comment>
  </owl:DatatypeProperty>

  <owl:DatatypeProperty rdf:about="http://purl.org/eventsoo/state">
    <rdfs:comment xml:lang="en">The state of the city in which the event takes place.</rdfs:comment>
  </owl:DatatypeProperty>

  <owl:DatatypeProperty rdf:about="http://purl.org/eventsoo/submittedPapers">
    <rdfs:domain rdf:resource="https://w3id.org/scholarlydata/ontology/conference-ontology.owl#AcademicEvent"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#unsignedInt"/>
    <rdfs:comment xml:lang="en">The number of submitted papers to the event.</rdfs:comment>
  </owl:DatatypeProperty>

  <owl:Class rdf:about="http://purl.org/eventsoo/Symposium">
    <rdfs:subClassOf rdf:resource="https://w3id.org/scholarlydata/ontology/conference-ontology.owl#AcademicEvent"/>
  </owl:Class>

  <owl:Class rdf:about="http://purl.org/eventsoo/SymposiumSeries">
    <rdfs:subClassOf rdf:resource="https://w3id.org/scholarlydata/ontology/conference-ontology.owl#EventSeries"/>
  </owl:Class>

</rdf:RDF>